   <!-- Page Wrapper -->
  <div id="wrapper">

   {{> menu}}

    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">

      <!-- Main Content -->
      <div id="content">

       {{> cabezote}}
 
 
 
 
                <div class="container-fluid">


                        <!-- DataTales Example -->
                        <div class="card shadow mb-4">
                            <div class="card-header py-3">
                            <h6 class="m-0 font-weight-bold text-primary">CV Empleadas</h6>
                            </div>
                            <div class="card-body">
                            <div class="table-responsive">
                                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                                <thead>
                                    <tr>
                                    <th>Nombre Completo</th>
                                    <th>DNI</th>
                                    <th>Ciudad</th>
                                    <th>Edad</th>
                                    <th>CV</th>
                                    <th>Acciones</th>
                                    </tr>
                                </thead>
                                {{#each datos}}
                                <tbody>
                                    <tr>
                                    <td>{{nombre}}</td>
                                    <td>{{dni}}</td>
                                    <td>{{ciudad}}</td>
                                    <td>{{edad}}</td>
                                    <td><a href="{{path}}" class="btn btn-success btn-block btn-sm" download>{{originalname}}</a></td>
                                    <td>
                                        <button class="btn btn-danger btn-sm"><i class="fas fa-trash-alt"></i></button>
                                    </td>
                                    </tr>
                                  
                                </tbody>
                                {{/each}}
                                </table>
                            </div>
                            </div>
                        </div>

                </div>
                        <!-- /.container-fluid -->


       </div>
      <!-- End of Main Content -->

      <!-- Footer -->
      {{>footer}}

    </div>
    <!-- End of Content Wrapper -->

  </div>
  <!-- End of Page Wrapper -->

