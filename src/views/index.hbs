{{> navegador}}

  <!-- Header -->
  <header class="masthead d-flex">
    <div class="container text-center my-auto">
      <h1 class="mb-1">Academia de Niñeras</h1>
      <h3 class="mb-5">
        <em>Plataforma para la capacitación de Empleadas Domésticas</em>
      </h3>
      <a class="btn btn-primary btn-xl js-scroll-trigger" href="/users/signup">Crear una cuenta</a>
    </div>
    <div class="overlay"></div>
  </header>

  <!-- About -->
  <section class="content-section bg-light" id="about">
    <div class="container text-center">
      <div class="row">
        <div class="col-lg-10 mx-auto">
          <h2>¡Bienvenidos!</h2>
          <p class="lead mb-5"> La agencia Niñeras al Rescate desarrollo una plataforma de capacitación para las empleadas domésticas de la ciudad de Rio Gallegos, de manera GRATUITA
           </p>
          <a class="btn btn-dark btn-xl js-scroll-trigger" href="http://niñerasalrescate.com" target="_blank">Ir a pagina principal</a>
        </div>
      </div>
    </div>
  </section>

  <!-- Services -->
  <section class="content-section bg-primary text-white text-center" id="services">
    <div class="container">
      <div class="content-section-heading">
        <h3 class="text-secondary mb-0">Servicios</h3>
        <h2 class="mb-5">Lo que ofrecemos</h2>
      </div>
      <div class="row">
        <div class="col-lg-4 col-md-6 mb-5 mb-lg-0">
          <span class="service-icon rounded-circle mx-auto mb-3">
            <i class="fas fa-female"></i>
          </span>
          <h4>
            <strong>Niñeras al Rescate</strong>
          </h4>
          <p class="text-faded mb-0">Niñeras capacitadas especializadas en el cuidado minucioso del niño</p>
        </div>
        <div class="col-lg-4 col-md-6 mb-5 mb-lg-0">
          <span class="service-icon rounded-circle mx-auto mb-3">
            <i class="fas fa-baby-carriage"></i>
          </span>
          <h4>
            <strong>Nanny Services</strong>
          </h4>
          <p class="text-faded mb-0">Amas de casa dedicadas a la limpieza del hogar y al cuidado del niño</p>
        </div>
        <div class="col-lg-4 col-md-6 mb-5 mb-lg-0">
          <span class="service-icon rounded-circle mx-auto mb-3">
            <i class="fas fa-home"></i>
          </span>
          <h4>
            <strong>Limpieza Home</strong>
          </h4>
          <p class="text-faded mb-0">Empleadas domésticas dedicadas a la limpieza del hogar.</p>
        </div>
       
      </div>
    </div>
  </section>


  <!-- Portfolio -->
  <section class="content-section" id="portfolio">
    <div class="container">
      <div class="content-section-heading text-center">
        <h3 class="text-secondary mb-0">Se parte de nuestro Team</h3>
        <h2 class="mb-5">¡Comienza hoy!</h2>
      </div>
      <div class="row no-gutters">
        <div class="col-lg-6">
          <a class="portfolio-item" href="/users/signup">
            <span class="caption">
              <span class="caption-content">
                <h2>Crear cuenta</h2>
                <p class="mb-0">Registrese y empieze a realiza nuestro curso de capacitación totalmente gratuito!</p>
              </span>
            </span>
            <img class="img-fluid" src="/img/imagen1.jpg" alt="">
          </a>
        </div>
        <div class="col-lg-6">
          <a class="portfolio-item" href="/users/signin">
            <span class="caption">
              <span class="caption-content">
                <h2>Iniciar Sesión</h2>
                <p class="mb-0">Ingrese a sitio y continue con el curso! ¡Ánimos!</p>
              </span>
            </span>
            <img class="img-fluid" src="/img/imagen2.jpg" alt="">
          </a>
        </div>
       
      </div>
    </div>
  </section>


  <!-- Map -->
  <section id="contact" class="map">
    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d19814.982304155452!2d-69.23828200445698!3d-51.62553942079886!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0xbdb6f952b21c66e5%3A0xab7c41f247647102!2sR%C3%ADo%20Gallegos%2C%20Santa%20Cruz!5e0!3m2!1ses!2sar!4v1578609294087!5m2!1ses!2sar" width="100%" height="100%" frameborder="0" style="border:0;" allowfullscreen=""></iframe>
    <br />
  </section>

    {{> footer-2}}
  <!-- Scroll to Top Button-->
  <a class="scroll-to-top rounded js-scroll-trigger" href="#page-top">
    <i class="fas fa-angle-up"></i>
  </a>
